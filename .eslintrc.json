{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "eslint.workingDirectories": [
        {"mode": "auto"}
    ],
    "extends": "plugin:react/recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": "warn",
        "camelcase": "error",
        "curly": "warn",
        "no-unused-vars": "warn",
        "quotes": ["warn", "double"]
    }
}
